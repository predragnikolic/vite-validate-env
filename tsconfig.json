{
   "compilerOptions": {
      "lib": ["es2022"],
      "module": "node16",
      "target": "es2022",
      "strict": true,
      "allowJs": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "emitDeclarationOnly": true,
      "declaration": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node16"
   }
}
